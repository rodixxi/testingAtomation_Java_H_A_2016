			<script type="text/javascript">
					$(document).ready(function(){

							var max_chars = 300;

							$('#max').html(max_chars);
							$('#comentario').keyup(function() {
									var chars = $(this).val().length;
									var diff = max_chars - chars;
									$(".disponible").html("<b>Caracteres disponibles</b>");
									$('#contador').html(diff);   
							});
					});
			</script>
										
