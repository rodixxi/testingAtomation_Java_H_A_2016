<script type="text/javascript" src="index_files/carouselTiny.js"></script><script src="index_files/plugins.js"></script><script>$(document).ready(function(){
    $('.herra li:not(.off):first').addClass('selected');
    
    $('ul.he:gt(0)').hide();
    
    $('.herra li:not(.off)').click(function(){
        $('.herra li:not(.off)').removeClass('selected');
        $(this).addClass('selected');
        
        var i = $('.herra li:not(.off)').index($(this));
        $('ul.he').hide();
        $('ul.he:eq('+i+')').show();
    });
    /* Reponsive */
    $(".mmInt").click(function() {
        $(".int aside .navAside").fadeToggle("slow", "linear");
    });
});</script><div id="notify"></div></body></html>