<script language="JavaScript" type="text/javascript">


function leerCookie(nombre) {
   a = document.cookie.substring(document.cookie.indexOf(nombre + '=') + nombre.length + 1,document.cookie.length);
   if(a.indexOf(';') != -1)a = a.substring(0,a.indexOf(';'))
   return a; 
}

</script>

<script type="text/javascript">
function focusUs(){
element = document.getElementById('us');
if(element.value == 'E-mail'){
element.value = '';
element.style.color = '#003366';
}
}


function blurUs(){
element = document.getElementById('us');
if(element.value == ''){
element.value = 'E-mail';
element.style.color = '#aaa';
}
}

function focusPw(){
element = document.getElementById('pw');
substitute = document.getElementById('pw1');
if((element.type == 'text') && (element.value == 'Clave') ){

element.style.display = 'none';
substitute.style.display = 'block';
substitute.style.color = '#003366';
substitute.focus();
substitute.value = '';
}
}


function blurPw(){
element = document.getElementById('pw');
substitute = document.getElementById('pw1');
if((substitute.value == '') && (substitute.type == 'password')){
element.value = 'Clave';
element.style.display = 'block';
element.style.color = '#aaa';
substitute.style.display = 'none';
substitute.value = '';
}
}

</script>


    
